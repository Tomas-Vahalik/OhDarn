  function outline(){
     
      $im = imagecreatetruecolor(1000, 500);
      $white = imagecolorallocate($im, 255, 255, 255);
      
      //imagefilledrectangle($im, 4, 4,50, 25, $white);
      imageline($im, 50, 25, 50, 400, $white);
      imageline($im, 40, 390, 900, 390, $white);
      
      imagepng($im);
      imagedestroy($im);
  }
  
  function spocitej($dat){
       sort($dat);
       
        $pocet = count($dat);
        $max = $dat[$pocet];
        $min = $dat[0];       
        $med = $dat[$pocet/2];                
        $top = $dat[$pocet/4 * 3];                   
        $bot = $dat[$pocet/4];  
     
  }
  
